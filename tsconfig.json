{
  "compilerOptions": {
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "noImplicitAny": true,
    "removeComments": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "target": "ES5"
  },
  "exclude": ["node_modules", "test", "dist", "**/*spec.ts"]
}
